generator client {
  provider = "prisma-client"
  output   = "../../../packages/prisma/client"
}
datasource db {
  provider = "mysql"
}

enum Role {
  ADMIN
  USER
}
model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  role      Role     @default(USER)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}
