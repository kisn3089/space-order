generator client {
  provider = "prisma-client"
  output   = "../../../packages/prisma/client"
}
datasource db {
  provider = "mysql"
}

enum UserRole {
  INTERN
  ENGINEER
  ADMIN
}
model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  role      UserRole     @default(INTERN)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("users")
}
