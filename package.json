{
  "name": "space-order",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev:order": "docker-compose -f apps/order/docker-compose.yml",
    "prod:order": "docker-compose -f apps/order/docker-compose.prod.yml -d",
    "dev:space": "docker-compose -f apps/space/docker-compose.yml"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
