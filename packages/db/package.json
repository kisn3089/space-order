{
  "name": "@spaceorder/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "private": true,
  "scripts": {
    "prisma:generate": "dotenv -e ../../apps/orderhub/.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../../apps/orderhub/.env -- prisma migrate dev",
    "prisma:studio": "dotenv -e ../../apps/orderhub/.env -- prisma studio",
    "prisma:seed": "dotenv -e ../../apps/orderhub/.env -- tsx prisma/seed.ts",
    "prisma:reset": "dotenv -e ../../apps/orderhub/.env -- prisma migrate reset",
    "build": "pnpm prisma:generate && tsc -b",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@spaceorder/auth": "workspace:*",
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0"
  },
  "devDependencies": {
    "@spaceorder/lintconfig": "workspace:*",
    "@spaceorder/tsconfig": "workspace:*",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.19.0",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "@types/bcrypt": "^6.0.0"
  }
}
